{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.0.15",
      "maxDuration": 30
    }
  },
  "buildCommand": "npm run build:vercel",
  "routes": [
    {
      "src": "/docs/(.*)",
      "dest": "/dist/public/swagger-ui/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}
